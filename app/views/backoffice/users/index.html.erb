<div id="page-wrapper">
  <div class="row">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h2>Listando Usuarios</h2>
      </div>
      
      <!-- /.panel-heading -->
      <div class="panel-body">
        <div class="apple_pagination">
          <%= will_paginate @users, :container => false %>
        </div>
        <div class="table-responsive">
          <table class="table table-striped table-bordered table-hover">
            <thead>
              <tr>
                <th>#</th>
                <th>Nome</th>
                <th>Email </th>
                <th>Tel. Celular</th>                
              </tr>
            </thead>
            <tbody>
             
              <% @users.each do |user| %>
                <tr>
                  <td>
                    <%= user.id %>
                  </td>
                  <td>
                    <%= user.nome %>
                  </td>
                  <td>
                    <%= user.email %>
                  </td>
                  <td>
                    <%= user.tel_cel %>
                  </td>
                </tr>
                <% end %>
              
            </tbody>
          </table>
        </div>
        <!-- /.table-responsive -->
      </div>
      <!-- /.panel-body -->
    </div>
  </div>
</div>